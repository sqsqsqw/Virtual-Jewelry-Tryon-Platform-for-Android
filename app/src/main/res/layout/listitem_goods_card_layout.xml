<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:roundiv="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <LinearLayout
        style="@style/GoodLinearLayoutL">
        <com.Hashqi.try_on_client.Component.SquareImageView
            style="@style/GoodImageView"
            android:id="@+id/LGoodImage"
            roundiv:left_top_radius="10dp"
            roundiv:right_top_radius="10dp"
            android:src="@drawable/default_good"
            />
        <TextView
            style="@style/GoodTextView"
            android:id="@+id/LGoodName"
            android:text="@string/good_name"
            />
    </LinearLayout>
</LinearLayout>
